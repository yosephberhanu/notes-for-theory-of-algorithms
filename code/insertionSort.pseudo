procedure insertionSort( A : list of sortable items )
 for j = 2 to length [A]
   do key = A[j]	
      // Insert A[j] into the sorted sequence A[1... j-1]
      i= j – 1
      while i>0 and A[i]>key
         do A[I + 1]←A[i]
         j = i–1
      end while
      
      A[i+1] = key
   end for
end procedure